<div class="centralize">
    <div class="container">
        <div class="profile-data">
            <figure class="profile-pic">
                <img src="/assets/imgs/animal-cat-cute-46024.jpg" />
            </figure>
            <div class="profile-name">
                <h1 class="username">{{data.nameNgo}}</h1>
                <div class="feed-header">
                    <div class="filters">
                        <ul class="group-categories">
                            {{#each causes}}
                                <a class="item-category category-filter filter-active" href="#">
                                    <li>{{descCategory}}</li>
                                </a>
                            {{/each}}
                        </ul>
                    </div>
                </div>
            </div>
            <figure class="linkSettings">
                {{!-- <img src="/assets/imgs/settings.svg" /> --}}
            </figure>
            <div class="profile-location-since">
                <h3 class="content">{{data.cityNgo}}. Desde {{data.createdAt.month}} de {{data.createdAt.year}}</h3>
            </div>
            <div class="profile-about">
                <p class="content">{{data.descriptionNgo}}</p>
            </div>
        </div>
        <div class="profile-stats">
            <ul>
                <li><span class="number">4,5/5</span>média de estrelas</li>
                <li><span class="number">8</span>Eventos realizados</li>
                <li><span class="number">41</span>Membros</li>
            </ul>
        </div>
