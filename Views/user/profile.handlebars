<link rel="stylesheet" href="/styles/css/homeUser.css" />
<link rel="stylesheet" href="/styles/css/profile.css" />

<div class="centralize">
    <div class="container">
        <div class="profile-data">
            <figure class="profile-pic availableVolunteer">
                <img src="/assets/imgs/user.svg" />
            </figure>
            <div class="profile-name">
                <h1 class="username">{{data.nameVolunteer}} {{data.lastNameVolunteer}}</h1>
                <div class="feed-header">
                    <div class="filters">
                        <ul class="group-categories">
                            {{#each causes}}
                                <a class="item-category category-filter filter-active" href="#">
                                    <li>{{descCategory}}</li>
                                </a>
                            {{/each}}
                        </ul>
                    </div>
                </div>
            </div>
            <a href="/user/settings">
                <figure class="linkSettings">
                    <img src="/assets/imgs/settings.svg" />
                </figure>
            </a>
            <div class="profile-location-since">
                <h3 class="content">{{data.cityVolunteer}}. Desde {{data.createdAt}}</h3>
            </div>
            <div class="profile-about">
                <p class="content">{{data.biographyVolunteer}}</p>
            </div>
        </div>
        <div class="profile-stats">
            <ul>
                <li><span class="number">{{data.averageStarsVolunteer}}/5</span>média de estrelas</li>
                <li><span class="number">12</span> ações em eventos</li>
                <li><span class="number">4</span> ONGs é inscrito</li>
            </ul>
        </div>
        <div class="group-nav">
            <nav class="nav-articles">
                <h1 class="title-nav">Eventos participados</h1>
                <div class="group-articles">
